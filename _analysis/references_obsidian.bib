@article{Gerrits2020,
  title = {Individual, Institutional, and Scientific Environment Factors Associated with Questionable Research Practices in the Reporting of Messages and Conclusions in Scientific Health Services Research Publications},
  author = {Gerrits, Reinie G. and Mulyanto, Joko and Wammes, Joost D. and {van den Berg}, Michael J. and Klazinga, Niek S. and Kringos, Dionne S.},
