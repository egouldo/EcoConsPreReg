@article{Knight2008,
  title = {Knowing but Not Doing: Selecting Priority Conservation Areas and the Research-Implementation Gap.},
  author = {Knight, A. T. and Cowling, R. M. and Rouget, M. and Balmford, A. and Lombard, A. T. and Campbell, B. M.},
  year = {2008},
